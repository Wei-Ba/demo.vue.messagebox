{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "tsBuildInfoFile": "node_modules/.cache/tsconfig.app.tsbuildinfo",
    "baseUrl": "./src",
    "paths": {
      "@/*": [
        "*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@components": [
        "components"
      ],
      "@views/*": [
        "views/*"
      ],
      "@views": [
        "views"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@utils": [
        "utils"
      ]
    }
  },
  "include": ["src/**/*"]
}